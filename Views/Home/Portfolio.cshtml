@{
    ViewBag.Title = "Portfolio";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div id="wrapper">
    <!--================= content-holder start ================-->
    <div class="content-holder" id="dunamic-content">
        <div class="container">
            <!--content Start -->
            <section class="content">
                <div class="anim-content">
                    <div class="page-box">
                        <div class="grid-full">
                            <h3 class="section-title">Project title</h3>
                            <div class="clear"></div>
                            <div class="separator"></div>
                        </div>
                        <div class="grid-full">
                            <div class="single-media">
                                <div id="single-media" class="owl-carousel">
                                    <!-- 1 slide -->
                                    <div class="item">
                                       
                                        <img src="~/images/photos/image1.PNG" class="respimg" alt="">
                                    </div>
                                    <!-- 2 slide -->
                                    <div class="item">
                                        <img src="~/images/photos/image2.PNG" class="respimg" alt="">
                                    </div>
                                    <div class="item">
                                        <img src="~/images/photos/image3.PNG" class="respimg" alt="">
                                    </div>
                                </div>
                                <div class="customNavigation">
                                    <a class="prev-slide transition"><i class="fa fa-angle-left"></i></a>
                                    <a class="next-slide transition"><i class="fa fa-angle-right"></i></a>
                                </div>
                            </div>
                        </div>
                        <div class="grid-full">
                            <div class="separator float-left"></div>
                        </div>
                        <div class="clear"></div>
                        <div class="grid-half">
                           
                        </div>
                        <div class="grid-half">
                            <ul class="project-info standart-box">
                                <li class="standart-box">
                                    <span>Category</span>
                                    <h4>Desktop Application</h4>
                                </li>
                                <li class="standart-box">
                                    <span>Date</span>
                                    <h4>06.05.2014</h4>
                                </li>
                                <li class="standart-box">
                                    <span>Client </span>
                                    <h4>PharamacyDealer</h4>
                                </li>
                                <li class="standart-box">
                                    <span>Address </span>
                                    <h4>egowoker.com</h4>
                                </li>
                            </ul>
                        </div>
                        
                    </div>
                </div>
            </section>
            <!--content end -->
        </div>
        <div class="clear"></div>
        <!--================= Footer start ================-->
        <div class="container">
            <footer>
                <div class="footer-logo"></div>
                <div class="grid-full">
                    <div class="policy-box">
                        <p>&copy; 2016 All Rights Reserved by U&S</p>
                    </div>
                    <div class="to-top-box"><a href="#main" class="scroll-btn transition"><i class="fa fa-angle-up"></i></a></div>
                </div>
            </footer>
        </div>
        
    </div>
    <!-- Content-holder end-->
</div>